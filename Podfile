  pod 'Kingfisher', '~> 3.0'
  pod 'Kanna', '~> 2.1.0'
  pod 'PromiseKit'
  pod 'PromiseKit/Alamofire'

  target 'SetGovTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SetGovUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
